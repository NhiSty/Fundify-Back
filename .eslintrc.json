{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": "error",
        "quotes": ["error", "single"],
        "yoda": "error"
    },
    "ignorePatterns": [
        "node_modules/",
        "out/",
        "public/",
        "tests/"
    ] 
}
